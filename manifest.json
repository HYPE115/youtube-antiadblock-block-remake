{
  "manifest_version": 2,
  "name": "Youtube Anti Adblock Bypasser",
  "version": "1.0",

  "browser_specific_settings": {
    "gecko": {
      "id": "youtube-adblock-block@mimumusic.com"
    }
  },

  "description": "little extension to bypass the anti adblocker",

  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["main.js"]
    }
  ]
}
